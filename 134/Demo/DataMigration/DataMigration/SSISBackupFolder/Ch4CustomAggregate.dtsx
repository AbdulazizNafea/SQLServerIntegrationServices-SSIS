    End Sub

    Public Overrides Sub Input0_ProcessInput(ByVal Buffer As Input0Buffer)
        While Buffer.NextRow
            If Buffer.Country <> strCountry Then
                strCountry = Buffer.Country
                strGroupedEmails = Buffer.Email
                AggregatedEmailsBuffer.AddRow()
                AggregatedEmailsBuffer.Country = strCountry
                AggregatedEmailsBuffer.GroupEmails.AddBlobData(encoding.GetBytes(strGroupedEmails))
            Else
                'strGroupedEmails = strGroupedEmails & ";" & Buffer.Email
                AggregatedEmailsBuffer.GroupEmails.AddBlobData(encoding.GetBytes(";" & Buffer.Email))
            End If
        End While
        If Buffer.EndOfRowset Then
            AggregatedEmailsBuffer.SetEndOfRowset()
        End If
    End Sub

