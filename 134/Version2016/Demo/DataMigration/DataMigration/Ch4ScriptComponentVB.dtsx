<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute> _
<CLSCompliant(False)> _
Public Class ScriptMain
    Inherits UserComponent

    Public Overrides Sub CreateNewOutputRows()
        ' Add rows by calling AddRow method on member variable called "<Output Name>Buffer"
        ' E.g., MyOutputBuffer.AddRow() if your output was named "My Output"

        Dim srdReader As New StreamReader(Me.Connections.QuarterFigures.ConnectionString), strCurrentLine As String
        While srdReader.Peek <> -1 
            strCurrentLine = srdReader.ReadLine
            If strCurrentLine.StartsWith("Begin Record") Then
                QuarterEndFiguresBuffer.AddRow()
            ElseIf strCurrentLine.StartsWith("Sales Order ID:") Then
                QuarterEndFiguresBuffer.SalesOrderID = CType(strCurrentLine.Remove(0, 16), Integer)
            ElseIf strCurrentLine.StartsWith("Employee:") Then
                QuarterEndFiguresBuffer.EmployeeName = strCurrentLine.Remove(0, 10)
            ElseIf strCurrentLine.StartsWith("Currency Code:") Then
                QuarterEndFiguresBuffer.CurrencyCode = strCurrentLine.Remove(0, 15)
            ElseIf strCurrentLine.StartsWith("Order Total:") Then
                QuarterEndFiguresBuffer.OrderTotal = CType(strCurrentLine.Remove(0, 13), Decimal)
            End If
        End While
        QuarterEndFiguresBuffer.SetEndOfRowset()
    End Sub
End Class

